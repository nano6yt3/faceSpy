{
  "manifest_version": 2,

  "name": "faceSpy",
  "description": "This extension parses Facebook for employees' data",
  "version": "1.0",
  
 
  
  "content_scripts": [{
    "matches": [ "http://*.facebook.com/*", "https://*.facebook.com/*"],
    "js": ["jquery.min.js","content.js"],
    "run_at": "document_end"
  }],

  "browser_action": {
    "default_icon": "icon1.png",
    "default_title": "faceSpy",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}